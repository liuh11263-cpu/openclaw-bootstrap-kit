{
  "agents": {
    "defaults": {
      "workspace": "${OPENCLAW_WORKSPACE}",
      "model": {
        "primary": "${MODEL_DAILY}",
        "fallbacks": ["${MODEL_CHEAP}"]
      },
      "contextTokens": 150000,
      "maxConcurrent": 2
    },
    "subagents": {
      "maxConcurrent": 3
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "${GATEWAY_BIND}",
    "port": ${GATEWAY_PORT},
    "auth": {
      "token": "__SET_IN_REAL_DEPLOYMENT__"
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "allowlist": ["${TELEGRAM_ALLOWED_CHAT}"]
    }
  },
  "memory": {
    "backend": "qmd"
  }
}
